<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
"http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
    <interface name="com.unikylin.StartMenuS">
        <method name="SearchApp" >
            <arg direction="in"  name="keyword" type="s"/>
            <arg direction="out" name="desktop_ids" type="as"/>
        </method>

        <method name="AddFavoriteApp">
            <arg direction="in"  name="desktop_id" type="s"/>
            <arg direction="out" name="success" type="b"/>
        </method>
        <method name="DelFavoriteApp">
            <arg direction="in"  name="desktop_id" type="s"/>
            <arg direction="out" name="success" type="b"/>
        </method>

        <method name="AddCategoryApp">
            <arg direction="in"  name="category" type="s"/>
            <arg direction="in"  name="desktop_id" type="s"/>
            <arg direction="out" name="success" type="b"/>
        </method>
        <method name="DelCategoryApp">
            <arg direction="in"  name="category" type="s"/>
            <arg direction="in"  name="desktop_id" type="s"/>
            <arg direction="out" name="success" type="b"/>
        </method>
        <method name="MoveCategoryApp">
            <arg direction="in"  name="src_category" type="s"/>
            <arg direction="in"  name="dst_category" type="s"/>
            <arg direction="in"  name="desktop_id" type="s"/>
            <arg direction="out" name="success" type="b"/>
        </method>
        <method name="GetCategoryApps">
            <arg direction="in"  name="category" type="s"/>
            <arg direction="out" name="desktop_ids" type="as"/>
        </method>
        <method name="GetAllCategoryApps">
            <arg direction="out" name="category_apps" type="a{sv}"/>
        </method>

        <method name="FocusApp">
            <arg direction="in"  name="desktop_id" type="s"/>
        </method>
        <method name="GetNFrequentApps">
            <arg direction="in"  name="top_n" type="i"/>
            <arg direction="out" name="most_frequent_apps" type="as"/>
        </method>
        <method name="ResetFrequentApps">
            <arg direction="out" name="success" type="b"/>
        </method>

        <method name="GetNNewApps" >
            <arg direction="in"  name="top_n" type="i"/>
            <arg direction="out" name="most_new_apps" type="as"/>
        </method>

        <method name="GetAllSortedApps" >
            <arg direction="out" name="all_sorted_apps" type="as"/>
        </method>

        <property name="FrequentApps" type="a{sv}" access="read">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="invalidates"/>
        </property>
        <property name="FavoriteApps" type="as" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
        </property>
    </interface>
</node>

