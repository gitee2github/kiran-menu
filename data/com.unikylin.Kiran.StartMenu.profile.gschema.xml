<schemalist>
    <enum id="com.unikylin.Kiran.StartMenu.DisplayMode">
    <value nick="compact" value="0"/>
    <value nick="expand" value="1"/>
    </enum>
    <enum id="com.unikylin.Kiran.StartMenu.DefaultPage">
    <value nick="favorite apps" value="0"/>
    <value nick="all apps" value="1"/>
    </enum>
  <schema id="com.unikylin.Kiran.StartMenu.profile" path="/com/unikylin/kiran/StartMenu/profile/">
    <key name="background-opacity" type="d">
      <default>1.0</default>
      <summary>Background Opacity of start menu</summary>
    </key>
    <key name="display-mode" enum="com.unikylin.Kiran.StartMenu.DisplayMode">
      <default>'compact'</default>
      <summary>Display mode for menu. 'compact' or 'expand'</summary>
    </key>
    <key name="default-page" enum="com.unikylin.Kiran.StartMenu.DefaultPage">
        <default>'favorite apps'</default>
        <summary>The default page when display in compact mode</summary>
    </key>
  </schema>
</schemalist>

